{
	"name": "phaser3-typescript-template",
	"version": "1.0.0",
	"description": "A template project for TypeScript and Parcel",
	"scripts": {
		"start": "parcel src/index.html -p 8080",
		"build": "parcel build src/index.html",
		"lint": "eslint ./src --ext .ts,.tsx"
	},
	"author": "scottwestover",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/scottwestover/phaser3-typescript-template.git"
	},
	"homepage": "https://github.com/scottwestover/phaser3-typescript-template",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "4.23.0",
		"@typescript-eslint/parser": "4.23.0",
		"cssnano": "5.0.7",
		"eslint": "7.26.0",
		"eslint-config-airbnb-base": "14.2.1",
		"eslint-config-airbnb-typescript": "12.3.1",
		"eslint-plugin-import": "2.22.1",
		"parcel": "2.0.0-beta.2",
		"parcel-plugin-clean-easy": "1.0.2",
		"parcel-plugin-static-files-copy": "2.6.0",
		"postcss": "8.2.15",
		"typescript": "4.2.4"
	},
	"dependencies": {
		"phaser": "3.55.2"
	},
	"resolutions": {
		"postcss": "8.2.15"
