{
	"name": "phaser3-typescript-template",
	"version": "1.0.0",
	"description": "A template project for TypeScript and Parcel",
	"scripts": {
		"start": "parcel src/index.html -p 8080",
		"build": "parcel build src/index.html --out-dir dist",
		"lint": "eslint ./src --ext .ts,.tsx"
	},
	"author": "scottwestover",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/scottwestover/phaser3-typescript-template.git"
	},
	"homepage": "https://github.com/scottwestover/phaser3-typescript-template",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "4.23.0",
		"@typescript-eslint/parser": "4.23.0",
		"eslint": "7.26.0",
		"eslint-config-airbnb-base": "14.2.1",
		"eslint-config-airbnb-typescript": "12.3.1",
		"eslint-plugin-import": "2.22.1",
		"parcel-plugin-clean-easy": "1.0.2",
		"parcel-plugin-static-files-copy": "2.6.0",
		"typescript": "4.2.4"
	},
	"dependencies": {},
	"resolutions": {
		"@typescript-eslint/eslint-plugin/**/lodash": "4.17.21",
    "eslint-plugin-import/**/hosted-git-info": "3.0.8"
  },
	"parcelCleanPaths": [
		"dist"
	],
	"staticFiles": {
		"staticPath": "public",
		"watcherGlob": "**"
	},
	"browserslist": [
		"last 1 Chrome versions"
	]
}
